permutationTestPerItem <-
function(idList, maxD, maxR){
# input a list of IDs, compute a pair of diagrams and compute their bottleneck and wasserstein distances
n = length(idList)
dgmDistanceFile = "./dgm-distance.txt"
sink(dgmDistanceFile)
for (i in 1:n){
id = idList[i]
#~/Documents/PhD/Research/TopologicalComputing/BrainNetworks/BrainTDA/R/
idStr = toString(id)
dataFileStr = paste("./InputDataSample/", idStr, "_distMat_c0.csv",sep = "")
data <- read.table(dataFileStr, sep=",", quote="\"", comment.char="")
dist= as.matrix(data)
Diag1= ripsDiag(X=dist, maxdimension=maxD, maxscale=maxR, library="GUDHI", printProgress=FALSE)$diagram
dataFileStr = paste("./InputDataSample/", idStr, "_distMat_c1.csv",sep = "")
data <- read.table(dataFileStr, sep=",", quote="\"", comment.char="")
dist= as.matrix(data)
Diag2= ripsDiag(X=dist, maxdimension=maxD, maxscale=maxR, library="GUDHI", printProgress=FALSE)$diagram
bDistance = bottleneck(Diag1, Diag2, dimension=0)
wDistance = wasserstein(Diag1, Diag2, p=2, dimension=0)
cat(id, bDistance, wDistance,"\n", append = TRUE)
plot(Diag1, barcode = TRUE)
}
sink()
# all data located in sci acount at samlev@shell.sci.utah.edu  in directory below:
#/home/sci/eleswong/Public/ForBei/permutationTest/
}
plot(Diag1,barcode = TRUE)
